<?xml version="1.0" encoding="utf-8"?>
<openerp>
	<data>
	
		<!-- FORM -->	
	    <record id="jsonrpc_keys_form" model="ir.ui.view">
	        <field name="name">JSON-RPC Keys</field>
	        <field name="model">jsonrpc.keys</field> 
	        <field name="type">form</field>       
	        <field name="arch" type="xml">
	           	<form string="JSON-RPC Key">
	           		<group name="jsronrpc_key" string="JSON-RPC Key">
		           		<field name="key" />
			        	<field name="user_id" />
			        	<field name="actived" />
			        	<field name="urls" class='oe_styling_v8' widget="text" />
		        	</group>
	           	</form>
	        </field>
	    </record>
	    
		<!-- TREE -->
	 	<record id="jsonrpc_keys_tree" model="ir.ui.view">
	 		<field name="name">JSON-RPC Keys</field>
	 		<field name="model">jsonrpc.keys</field>	 		
			<field name="type">tree</field>
			<field name="arch" type="xml">
				<tree string="JSON-RPC Keys">
		           	<field name="key" />
			        <field name="user_id" />
			        <field name="urls" />
				</tree>
			</field>
	</record>
	
    </data>
</openerp>